{% extends 'InformesBundle::FichaClinica/sublayout.html.twig' %}
{% block body %}
<div id="breadcrumbs" class="navbar-fixed-top">
	<ul class="breadcrumb">
		<li>
            <i class="icon-home"></i> <a href="{{ path('Dashboard_home') }}">Home</a>
            <span class="divider"><i class="icon-angle-right"></i></span>
        </li>
        <li>
            <a href="{{ path('informesInicio') }}">Informes</a>
            <span class="divider"><i class="icon-angle-right"></i></span>
        </li>
        <li>
            {{ nombreModulo }}
        </li>
    </ul>
</div>
{# Informes default #}
<div class="well">
    <h3 class="grey lighter smaller">
        Seleccionar un Informe
    </h3>
    <hr>
    <div class="row-fluid">
        {% if app.user.tienePerfil('INFORMES_FICHACLINICA_INTERCONSULTASREALIZADAS') %}
        <div class="span4 well well-small well-moduloRebsol">
            <div class="row-fluid">
                <div class="span12">
                    <a href="{{ path('Informes_FichaClinica_InterconsultasRealizadas_inicio') }}">
                        <h3><i class="icon-comments-alt"></i> Interconsultas Realizadas</h3>
                    </a>
                    <span>
                        Listado de interconsultas realizadas entre un rango de fechas especifico,
                        indicando sucursal, especialidad, profesional etc...
                    </span>
                </div>
            </div>
        </div>
        {% endif %}
    </div>
    <hr>
</div>
{% endblock %}